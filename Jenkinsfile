pipeline {
    agent any
    stages {
        stage('Working with Jenkins){
            steps {
                script {
                    var1 == 10
                    if (var1 == 10) {
                        println "my var1 is 10"
                    }
                    else {
                        println "my var1 value is not 10"
                    }
                }
            }
        }
    }
}
